{
  "surround_pairs_for_replacement": {
    "{": ["{ ", " }"],
    "}": ["{", "}"],
    "[": ["[ ", " ]"],
    "]": ["[", "]"],
    "(": ["( ", " )"],
    ")": ["(", ")"],
    "<": ["< ", " >"],
    ">": ["<", ">"],
    "<!--": ["<!-- ", " -->"],
    "-->": ["<!--", "-->"],
    "/*": ["/* ", " */"],
    "*/": ["/*", "*/"]
  },
  "surround_pairs_for_search": {
    "{": ["\\{[\\s]?", "[\\s]?s*\\}"],
    "}": ["{", "}"],
    "[": ["\\[[\\s]?", "[\\s]?\\]"],
    "]": ["[", "]"],
    "(": ["\\([\\s]?", "[\\s]?\\)"],
    ")": ["(", ")"],
    "<": ["<[\\s]?", "[\\s]?>"],
    ">": ["<", ">"],
    "<!--": ["<!--[\\s]?", "[\\s]?-->"],
    "-->": ["<!--", "-->"],
    "/*": ["\\/\\*", "\\*\\/"],
    "*/": ["/*", "*/"]
  }
}
